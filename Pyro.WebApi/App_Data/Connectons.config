  <!-- ############ ReadMe ##############################################################
       The Pyro FHIR Server can use either 'Microsoft SQL Server' or 'PostgreSQL' as its database.
       Below are is the syntax for each. Please note that only one of each can be ative at any one time.
       The server will not start if they are more than one.
       
       Microsoft SQL Server
       <add name="PyroConnectionString" 
       connectionString="[Your Microsoft SQL Server connection string]" 
       providerName="System.Data.SqlClient" />
       
       PostgreSQL
       <add name="PyroConnectionString"
       connectionString="[Your PostgreSQL connection string]"
       providerName="Npgsql" />
  -->

